QWidget#widget_title{
	background: qlineargradient(spread:padding-bottom, x1:0, y1:0, x2:0, y2:1, stop:0 #1B89A1, stop:1 #1077A0);
}
QLabel#lab_Ico,QLabel#lab_Title{
	border-radius:0px;
	color: #F0F0F0;
	background-color:rgba(0,0,0,0);
	border-style:none;
}
QPushButton#btnMenu_Close{
	border-radius:0px;
	color: #F0F0F0;
	background-color:rgba(0,0,0,0);
	border-style:none;
}

QPushButton#btnMenu_Close:hover{
	background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 rgba(238, 0, 0, 128), stop:1 rgba(238, 44, 44, 255));
}
QToolButton#m_ptbnClose{
	image:url(:/image/sysButton/close.png);
}
QToolButton#m_ptbnClose:hover{
	image:url(:/image/sysButton/close_hover.png);
}
QToolButton#m_ptbnClose:pressed{
	image:url(:/image/sysButton/close_pressed.png);
}

QToolButton#m_ptbnMix{
	image:url(:/image/sysButton/min.png);
}
QToolButton#m_ptbnMix:hover{
	image:url(:/image/sysButton/min_hover.png);
}
QToolButton#m_ptbnMix:pressed{
	image:url(:/image/sysButton/min_pressed.png);
}

QPushButton#transparentButton{
	background:transparent;
}

QPushButton#menuButton{
	background:transparent;
}
QPushButton#menuButton::menu-indicator{
	image:None;
}

QPushButton#trayButton{
	background:transparent; 
	color:white;
}

QPushButton#okButton{
	border:1px solid lightgray;
	background:rgb(230,230,230);
}
QPushButton#okButton:hover{
	border-color:green;
	background:transparent;
}

QPushButton#skinButton{
	border-radius:3px;
	border:1px solid rgb(180, 190, 200);
	color:rgb(70, 70, 70);
	background:transparent;
}

QPushButton#greenButton{
	border-radius:5px;
	background:rgb(110, 190, 10);
